  "scripts": {
    "build:api": "ts-node -r tsconfig-paths/register scripts/build/api",
    "build:dashboard": "ts-node -r tsconfig-paths/register scripts/build/dashboard",
    "start:api": "node .server/index",
    "dev:dashboard": "ts-node -r tsconfig-paths/register scripts/dev",
    "dev:api": "ts-node-dev -r tsconfig-paths/register src/index",
    "test": "ts-node -r tsconfig-paths/register scripts/test",
    "coverage": "nyc --reporter=lcov --reporter=text-summary ts-node -r tsconfig-paths/register scripts/test"
  },
