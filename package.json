    "gray-matter": "^4.0.2",
    "ioredis": "^4.19.4",
    "liquidjs": "^9.20.1",
    "marked": "^1.2.8",
    "mime-types": "^2.1.28",
    "monaco-editor": "^0.22.1",
    "pg": "^8.5.1",
    "preact": "^10.5.12",
    "ramda": "^0.27.1",
    "react-router-dom": "^5.2.0",
    "recharts": "^2.0.4",
