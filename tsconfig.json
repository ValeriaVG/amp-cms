{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "allowJs": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": "src",
    "outDir": ".server",
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxFragmentFactory": "h.Fragment",
    "noEmitOnError": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
